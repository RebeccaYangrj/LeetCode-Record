solved by myself!!!! celebrate!!!!


class Solution:
    def isSameTree(self, p: TreeNode, q: TreeNode) -> bool:
        if p is None and q:
            return False
        if q is None and p:
            return False
        if p is None and q is None:
            return True
        if p and q:
            if p.val != q.val:
                return False
            return self.isSameTree(p.right, q.right) and self.isSameTree(p.left, q.left)
        
